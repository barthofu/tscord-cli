{
  "name": "tscord-cli",
  "version": "1.0.0",
  "description": "A CLI for TSCord",
  "main": "lib/index.js",
  "scripts": {
    "build": "npm run build:clean && npm run type:check && npm run build:compile",
    "build:clean": "rimraf lib",
    "build:compile": "swc src -d lib",
    "type:check": "tsc --pretty --skipLibCheck --noEmit",
    "dev": "npm run build:clean && npm run build:compile && npm run dev:exec",
    "dev:exec": "node lib/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/barthofu/tscord-cli.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/barthofu/tscord-cli/issues"
  },
  "homepage": "https://github.com/barthofu/tscord-cli#readme",
  "dependencies": {
    "@octokit/request": "^6.2.1",
    "axios": "^0.27.2",
    "chalk": "^5.0.1",
    "commander": "^9.4.1",
    "inquirer": "^8.0.1",
    "node-plop": "~0.26.0",
    "plop": "^2.7.0",
    "tar": "^6.1.11"
  },
  "devDependencies": {
    "@types/node": "^18.7.23",
    "@types/tar": "^6.1.3",
    "rimraf": "^3.0.2",
    "swc": "^1.0.11",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.7.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.4"
  }
}
