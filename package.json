{
  "name": "tscord-cli",
  "version": "0.0.9",
  "description": "A CLI for TSCord",
  "main": "lib/index.js",
  "bin": {
    "tscord": "./lib/index.js"
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:clean && npm run type:check && npm run build:compile && npm run build:post",
    "build:clean": "rimraf lib",
    "build:compile": "swc ./src -d ./lib",
    "build:post": "ncp ./src/plop/cli/generators/templates ./lib/plop/cli/generators/templates",
    "type:check": "tsc --pretty --skipLibCheck --noEmit",
    "dev": "swc src -w -d lib",
    "start": "node lib/index.js",
    "prepublish": "npm run build",
    "test": "echo \"No test specified\" && exit 0",
    "release": "np --no-2fa"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/barthofu/tscord-cli.git"
  },
  "keywords": [],
  "author": {
    "name": "Bartholom√© Gili",
    "email": "dev.bartho@gmail.com"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/barthofu/tscord-cli/issues"
  },
  "homepage": "https://github.com/barthofu/tscord-cli#readme",
  "dependencies": {
    "@octokit/request": "^6.2.1",
    "axios": "^0.27.2",
    "boxen": "^5.1.2",
    "chalk": "^4.1.2",
    "commander": "^9.4.1",
    "inquirer": "^8.0.1",
    "node-plop": "~0.26.0",
    "oneline": "^1.0.3",
    "ora": "^5.1.2",
    "plop": "^2.7.0",
    "tar": "^6.1.11"
  },
  "devDependencies": {
    "@types/node": "^18.7.23",
    "@types/tar": "^6.1.3",
    "ncp": "^2.0.0",
    "np": "^7.6.2",
    "rimraf": "^3.0.2",
    "swc": "^1.0.11",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.7.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.4"
  }
}
